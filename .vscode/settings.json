{
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "eslint.alwaysShowStatus": true,
  "eslint.enable": true,
  "cSpell.language": "en",
  "eslint.options": {
    "configFile": ".eslintrc.js"
